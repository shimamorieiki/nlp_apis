{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(funcName)s:%(lineno)s / [%(levelname)s]: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "__main__": {
            "handlers": [
                "consoleHandler"
            ],
            "propagate": true
        },
        "src": {
            "handlers": [
                "consoleHandler"
            ],
            "propagate": true
        }
    },
    "root": {
        "level": "DEBUG"
    }
}