{
	"name": "cabocha",
	"dockerFile": "Dockerfile",
	"context": "..",
	"remoteUser": "docker",
	"customizations":{
		"vscode":{
			"extensions":[
				"magicstack.MagicPython",
				"ms-python.vscode-pylance",
				"ms-python.python",
				"donjayamanne.python-extension-pack",
				"humao.rest-client"
			]
		}
	},
	"postCreateCommand": "pip3 install -r requirements.txt"
}